{
  "name": "advanced-node",
  "version": "1.0.0",
  "description": "API em NodeJs usando Typescript, TDD, Clean Architecture, Design Patterns e SOLID principles",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:fb-api": "npm test -- -c ./jest.integration.config.js ./tests/external/facebook-api.test.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilmardanilo/advanced-node.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ilmardanilo/advanced-node/issues"
  },
  "homepage": "https://github.com/ilmardanilo/advanced-node#readme",
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.18",
    "@types/typescript": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^27.1.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "pg-mem": "^1.9.17",
    "ts-jest": "^27.0.5",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.29"
  }
}
